<?xml version="1.0" encoding="UTF-8"?>

<layer-list
	xmlns:android="http://schemas.android.com/apk/res/android">
	<item
		android:id="@android:id/background" >
		 <shape android:shape="rectangle"  >
         <corners android:radius="3dip" />
         <stroke android:width="1dip" android:color="@color/red_progressbar_seen" />
         <gradient android:angle="-90"
		 android:startColor="@color/red_progressbar_seen"
		 android:centerY="0.50"
		 android:endColor="@color/red_progressbar_seen" />
		</shape>
	</item>
	<item
		android:id="@android:id/secondaryProgress">
		<clip>
			 <shape android:shape="rectangle"  >
         <corners android:radius="3dip" />
				<gradient
					android:startColor="#000000"
					android:endColor="#000000"
					android:angle="270" />
			</shape>
		</clip>
	</item>
	<item
		android:id="@android:id/progress">
		<clip>
			 <shape android:shape="rectangle"  >
				<corners android:radius="3dip" />
				<gradient
					android:startColor="#26000000"
					android:endColor="#26000000"
					android:angle="270" />
			</shape>
		</clip>
	</item>
</layer-list>

